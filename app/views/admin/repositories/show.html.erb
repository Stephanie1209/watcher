<%= link_to 'Actualizar', admin_repository_path(@repository.name), method: :put %>

<h1> <%= @repository.name %> </h1>
<p> <b> stars: </b> <%= @repository.stars %> </p>
<p> <b> forks_count: </b><%= @repository.forks_count %> </p>
<p> <b> started at: </b><%= @repository.started_at.strftime("%d/%B/%Y") %> </p>
